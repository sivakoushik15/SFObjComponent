<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_8_Characters</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(AccountNumber)!= 8</errorConditionFormula>
    <errorDisplayField>AccountNumber</errorDisplayField>
    <errorMessage>Account number should contain 8 digits</errorMessage>
</ValidationRule>
